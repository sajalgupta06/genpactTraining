@model List<Course>

<h1 class="p-4">Courses</h1>

<h5>No Of Courses : @Model.Count</h5>
<table class="table table-bordered">
    <thead>
    <tr>
   <th>CID</th>
   <th>CName</th>
   <th>CFee</th>
   <th>Technology</th>
   <th>Status</th>
    </tr>
    </thead>
    <tbody>
            @foreach (var obj in Model)
            {<tr>

                <td>@obj.CourseId</td>
                <td>@obj.CName</td>
                <td>@obj.CFee</td>
                <td>@obj.Technology</td>
                <td>@obj.Status</td>
              
            </tr>
            }
        </tbody>
    
</table>